    "# import pyvis\n",
    "from pyvis.network import Network\n",
    "# create vis network\n",
    "net = Network(notebook=True, width=1000, height=600)\n",
    "# load the networkx graph\n",
    "net.from_nx(G)\n",
    "net.repulsion()\n",
    "# show\n",
    "net.show(\"example.html\")"
   ]
  },
