   ],
   "source": [
    "import networkx as nx\n",
    "\n",
    "G = nx.from_pandas_edgelist(df_2,\n",
    "                           source='home_team',\n",
    "                           target='away_team',\n",
    "                           edge_attr='dif_goals')\n",
    "\n",
    "print(\"No. of unique countries:\", len(G.nodes))\n",
    "print(\"No. of connections:\", len(G.edges))"
   ]
